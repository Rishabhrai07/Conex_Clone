pipeline {
  agent any
  stages {
    stage('checkout') {
      steps {
        echo 'checking out'
      }
    }

    stage('Install Dependencies') {
      steps {
        echo 'installing dependencies'
      }
    }

    stage('Lint and Test') {
      steps {
        echo 'testing'
      }
    }

    stage('Build') {
      steps {
        echo 'Building '
      }
    }

  }
}